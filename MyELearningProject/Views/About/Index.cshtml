@model MyELearningProject.DAL.Entities.About
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-block">
                Hakkımızda
                <br />
                <table class="table table-bordered">
                    <tr>
                        <th>#</th>
                        <th>Başlık</th>
                        <th>İçerik</th>              
                        <th>Güncelle</th>
                    </tr>
             
                        <tr>
                            <td>@Model.AboutID</td>
                            <td>@Model.Title</td>
                            <td>@Model.Content</td>
                           



                            <td><a href="/About/UpdateAbout/@Model.AboutID" class="btn btn-outline-warning">Güncelle</a></td>
                        </tr>
                    
                </table>
                
            </div>
        </div>
    </div>
</div>



